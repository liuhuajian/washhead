<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/color_f7f7f7">
    <include android:id="@+id/include_titleview"
        layout="@layout/include_titleview"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@mipmap/ic_arm"
        android:layout_marginTop="190dp"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_circle_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="79dp"
        android:layout_marginTop="436dp"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_circle_out"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_left"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_left"
            app:layout_constraintStart_toStartOf="@+id/container_circle_left"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_left"
            android:src="@mipmap/ic_circle_center"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_left"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_left"
            app:layout_constraintStart_toStartOf="@+id/container_circle_left"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_left"
            android:src="@mipmap/ic_circle_inner"
            />
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="肩松"
            android:textSize="30sp"
            android:textColor="@color/color_383838"
            app:layout_constraintTop_toTopOf="@id/container_circle_left"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_left"
            app:layout_constraintStart_toStartOf="@+id/container_circle_left"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_left"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_circle_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="436dp"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_circle_out"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_center"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_center"
            app:layout_constraintStart_toStartOf="@+id/container_circle_center"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_center"
            android:src="@mipmap/ic_circle_center"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_center"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_center"
            app:layout_constraintStart_toStartOf="@+id/container_circle_center"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_center"
            android:src="@mipmap/ic_circle_inner"
            />
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试"
            android:textSize="30sp"
            android:textColor="@color/color_383838"
            app:layout_constraintTop_toTopOf="@id/container_circle_center"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_center"
            app:layout_constraintStart_toStartOf="@+id/container_circle_center"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_center"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_circle_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="79dp"
        android:layout_marginTop="436dp"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_circle_out"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_right"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_right"
            app:layout_constraintStart_toStartOf="@+id/container_circle_right"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_right"
            android:src="@mipmap/ic_circle_center"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/container_circle_right"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_right"
            app:layout_constraintStart_toStartOf="@+id/container_circle_right"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_right"
            android:src="@mipmap/ic_circle_inner"
            />
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="肩紧"
            android:textSize="30sp"
            android:textColor="@color/color_383838"
            app:layout_constraintTop_toTopOf="@id/container_circle_right"
            app:layout_constraintBottom_toBottomOf="@+id/container_circle_right"
            app:layout_constraintStart_toStartOf="@+id/container_circle_right"
            app:layout_constraintEnd_toEndOf="@+id/container_circle_right"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_confirm_arm"
        android:layout_width="368dp"
        android:layout_height="86dp"
        app:layout_constraintTop_toBottomOf="@id/container_circle_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/bg_border_636363"
        android:text="肩宽确认"
        android:textColor="@color/color_969696"
        android:gravity="center"
        android:textSize="34sp"
        android:layout_marginTop="38dp"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_temperature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ic_temperature"
        app:layout_constraintTop_toBottomOf="@+id/tv_confirm_arm"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="55dp"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_confirm_temperature"
        android:layout_width="368dp"
        android:layout_height="86dp"
        app:layout_constraintTop_toBottomOf="@id/iv_temperature"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/bg_border_636363"
        android:text="水温确认"
        android:textColor="@color/color_969696"
        android:gravity="center"
        android:textSize="34sp"
        android:layout_marginTop="44dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>